{
  "Api": {
    "Port": 5123,
    "AllowedOrigins": [
      "https://salecast.net",
      "https://localhost:5221"
    ]
  },
  "Update": {
    "Url": "https://github.com/SaleCast/Salecast.Printer/releases/latest/download"
  },
  "Startup": {
    "RunAtBoot": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  }
}
